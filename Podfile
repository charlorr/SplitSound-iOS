target 'SplitSound' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'AudioKit'
  pod 'AudioKit/UI'
  # Pods for SplitSound

  target 'SplitSoundTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SplitSoundUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
